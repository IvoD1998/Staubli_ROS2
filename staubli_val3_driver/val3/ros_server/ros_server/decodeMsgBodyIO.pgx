          Staubli:writeSingIOAck.header.nReplyCode=SimpleMsg:ReplyCode.FAILURE
      //  break
      //  default
      //  break
      //endSwitch
      // jump to state 5: sendAck()
      x_nMsgRecvState=nAck
    else
      // reset state machine, since no ACK is required
      x_nMsgRecvState=nReset
    endIf
  endIf
end]]></Code>
