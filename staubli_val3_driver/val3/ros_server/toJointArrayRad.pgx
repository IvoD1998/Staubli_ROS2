<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="toJointArrayRad" access="public">
    <Parameters xmlns="http://www.staubli.com/robotics/VAL3/Param/1">
      <Parameter name="x_jJointRx" type="jointRx" xsi:type="element" use="reference" />
      <Parameter name="x_nJointArray" type="num" xsi:type="array" use="reference" />
    </Parameters>
    <Locals>
      <Local name="l_nDeg2Rad" type="num" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  // Copyright (c) 2016, Ocado Technology - Robotics Research Team
