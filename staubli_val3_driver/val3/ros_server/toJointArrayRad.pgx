  // limitations under the License.



  l_nDeg2Rad = 0.01745329251
  
  // assuming the remaining elements (6-9) have zeros assigned by default
  x_nJointArray[0] = x_jJointRx.j1 * l_nDeg2Rad
  x_nJointArray[1] = x_jJointRx.j2 * l_nDeg2Rad
  x_nJointArray[2] = x_jJointRx.j3 * l_nDeg2Rad
  x_nJointArray[3] = x_jJointRx.j4 * l_nDeg2Rad
  x_nJointArray[4] = x_jJointRx.j5 * l_nDeg2Rad
  x_nJointArray[5] = x_jJointRx.j6 * l_nDeg2Rad
end]]></Code>
  </Program>
</Programs>
