  x_nUserIn=dioGet(diUserIn)
  x_nValveOut=dioGet(diValveOut)
  
  if (bBasicIO1)
    x_nBasicIn=dioGet(diBasicIn)
    x_nBasicOut=dioGet(diBasicOut)
  else
    x_nBasicIn=0
    x_nBasicOut=0
  endIf
  
  if (bBasicIO2)
    x_nBasicIn2=dioGet(diBasicIn)
    x_nBasicOut2=dioGet(diBasicOut)
  else
    x_nBasicIn2=0
    x_nBasicOut2=0
  endIf
  
end]]></Code>
